<!DOCTYPE HTML>
<html>
	<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title><%= el.name %></title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="" />
	<meta name="keywords" content="" />
	<meta name="author" content="" />

  <!-- Facebook and Twitter integration -->
	<meta property="og:title" content=""/>
	<meta property="og:image" content=""/>
	<meta property="og:url" content=""/>
	<meta property="og:site_name" content=""/>
	<meta property="og:description" content=""/>
	<meta name="twitter:title" content="" />
	<meta name="twitter:image" content="" />
	<meta name="twitter:url" content="" />
	<meta name="twitter:card" content="" />

	<link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,700" rel="stylesheet">
	
	<link rel="icon" href="<%= el.url + el.favicon.url %>" type="image/png">

	<!-- Animate.css -->
	<link rel="stylesheet" href="css/animate.css">
	<!-- Icomoon Icon Fonts-->
	<link rel="stylesheet" href="css/icomoon.css">
	<!-- Bootstrap  -->
	<link rel="stylesheet" href="css/bootstrap.css">

	<!-- Magnific Popup -->
	<link rel="stylesheet" href="css/magnific-popup.css">

	<!-- Flexslider  -->
	<link rel="stylesheet" href="css/flexslider.css">

	<!-- Owl Carousel -->
	<link rel="stylesheet" href="css/owl.carousel.min.css">
	<link rel="stylesheet" href="css/owl.theme.default.min.css">
	
	<!-- Flaticons  -->
	<link rel="stylesheet" href="fonts/flaticon/font/flaticon.css">

	<!-- Theme style  -->
	<link rel="stylesheet" href="css/style.css">

	<!-- Modernizr JS -->
	<script src="js/modernizr-2.6.2.min.js"></script>
	<!-- FOR IE9 below -->
	<!--[if lt IE 9]>
	<script src="js/respond.min.js"></script>
	<![endif]-->

	</head>
	<body>
		
	<div class="colorlib-loader"></div>

	<div id="page">

 		<!-- Start header -->
		<%- include('components/header.ejs', {el: el} ) %>
		<!-- End header-->
		
		<aside id="colorlib-hero">
			<div class="flexslider">
				<ul class="slides">
					<% el.carousels.map((im) => {  %>
						<%- include('components/carousel_image.ejs', {
						url: el.url+im.img.url,
						header: el.lang== 'en' ? im.header : im.header_arabic ,
						paragraph: el.lang =='en'? im.paragraph : im.paragraph_arabic 
						}) %>
					<% })%>

			  	</ul>
		  	</div>
		</aside>


		<div id="colorlib-intro">
			<div class="container">
				<div class="row">
					<div class="col-md-12 tabulation animate-box">
						<ul class="nav nav-tabs"  dir="<%= el.lang == 'en' ? 'ltr' : 'rtl'  %>">
							<% el.home_blocks.map((tab,i) => {  %>
								<%- include('components/nav_tab.ejs', {
									title: el.lang == 'en' ? tab.title : tab.title_arabic ,
									 active: i == 0 ? 'active' : "" , 
									 href:"tab-"+i , el:el ,
									}) %>
							<% })%>
						</ul>
						<div class="tab-content">
							<% el.home_blocks.map((tab,i) => {  %>
								<%- include('components/nav_block.ejs', {
								rich_text:el.lang == 'en' ? tab.rich_text : tab.rich_text_arabic ,
								active: i == 0 ? 'active' : ""  , 
								href:"tab-"+i,
								 url: el.url + tab.image.url
								}) %>
							<% })%>
			         </div>
			     </div>
				</div>
			</div>
		</div>
		<div id="colorlib-project">
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-md-offset-2 text-center colorlib-heading animate-box">
						<h2><%= el.lang == 'en' ? 'Our Projects' : 'مشاريعنا' %></h2>
						<!-- Start our projects  -->
						<p><%= el.lang == 'en' ? el.our_projects : el.our_projects_arabic %></p>
						<!-- End our projects  -->
					</div>
				</div>
				<div class="row"  dir="<%= el.lang == 'en' ? 'ltr' : 'rtl'  %>">
					<% el.projects.map((project) => {  %>
						<%- include('components/project_card.ejs',{el:el , 
						current:false,
						name: el.lang == 'en' ? project.name : project.name_arabic , 
						col:project.col , id:project.id , 
						url: el.url + project.projectImages[0].url })  %>
					<% })%>					
				</div>
			</div>
		</div>


			<div id="colorlib-project">
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-md-offset-2 text-center colorlib-heading animate-box">
						<h2><%= el.lang == 'en' ? 'Current Projects' : 'مشاريعنا الحالية' %></h2>
						<!-- Start our projects  -->
						<p><%= el.lang == 'en' ? el.our_projects : el.our_projects_arabic %></p>
						<!-- End our projects  -->
					</div>
				</div>
				<div class="row"  dir="<%= el.lang == 'en' ? 'ltr' : 'rtl'  %>">
					<% el.currentProjects.map((project) => {  %>
						<%- include('components/project_card.ejs',{el:el , 
						current:true,
						name: el.lang == 'en' ? project.name : project.name_arabic , 
						col:project.col , id:project.id , 
						url: el.url + project.projectImages[0].url })  %>
					<% })%>					
				</div>
			</div>
		</div>

		<div id="colorlib-counter" class="colorlib-counters" style="background-image: url('<%= el.url + el.slider_image_1.url %>');" data-stellar-background-ratio="0.5">
			<div class="overlay"></div>
			<div class="container">
				<div class="row no-gutter">
					<div class="col-md-3 col-sm-6 aside-stretch text-center animate-box">
						<div class="counter-entry">
							<span class="icon icon-white"><i class="flaticon-skyline"></i></span>
							<span class="colorlib-counter js-counter" data-from="0" data-to="121" data-speed="5000" data-refresh-interval="50"></span>
							<strong style="color:white"><%= el.lang == 'en'?'Projects':'مشاريع'%></strong>
						</div>
					</div>
					<div class="col-md-3 col-sm-6 text-center animate-box">
						<div class="counter-entry">
							<span class="icon"><i class="flaticon-engineer"></i></span>
							<span class="colorlib-counter js-counter" data-from="0" data-to="18" data-speed="5000" data-refresh-interval="50"></span>
							<strong style="color:white"><%= el.lang == 'en'?'Empployess':'موظف'%></strong>
						</div>
					</div>
					<div class="col-md-3 col-sm-6 text-center animate-box">
						<div class="counter-entry">
							<span class="icon"><i class="flaticon-architect-with-helmet"></i></span>
							<span class="colorlib-counter js-counter" data-from="0" data-to="31" data-speed="5000" data-refresh-interval="50"></span>
							<strong style="color:white"><%= el.lang == 'en'?'Constructor':'بناء'%></strong>
						</div>
					</div>
					<div class="col-md-3 col-sm-6 text-center animate-box">
						<div class="counter-entry">
							<span class="icon"><i class="flaticon-worker"></i></span>
							<span class="colorlib-counter js-counter" data-from="0" data-to="6" data-speed="5000" data-refresh-interval="50"></span>
							<strong style="color:white"><%= el.lang == 'en'?'Partner':'شريك'%></strong>
						</div>
					</div>
				</div>
			</div>
		</div>

 		<!-- Start Footer -->
		<%- include('components/footer.ejs', {el: el} ) %>
		<!-- End Footrer-->

	</div>


	
	<!-- jQuery -->
	<script src="js/jquery.min.js"></script>
	<!-- jQuery Easing -->
	<script src="js/jquery.easing.1.3.js"></script>
	<!-- Bootstrap -->
	<script src="js/bootstrap.min.js"></script>
	<!-- Waypoints -->
	<script src="js/jquery.waypoints.min.js"></script>
	<!-- Stellar Parallax -->
	<script src="js/jquery.stellar.min.js"></script>
	<!-- Flexslider -->
	<script src="js/jquery.flexslider-min.js"></script>
	<!-- Owl carousel -->
	<script src="js/owl.carousel.min.js"></script>
	<!-- Magnific Popup -->
	<script src="js/jquery.magnific-popup.min.js"></script>
	<script src="js/magnific-popup-options.js"></script>
	<!-- Counters -->
	<script src="js/jquery.countTo.js"></script>
	<!-- Main -->
	<script src="js/main.js"></script>

	</body>
</html>
